{
  "name": "autonome-sales-agent",
  "version": "1.0.0",
  "description": "Unified agentic sales system for Autonome",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "cli": "tsx src/cli.ts",
    "discovery": "tsx src/agents/discovery/cli.ts",
    "research": "tsx src/agents/research/cli.ts",
    "outreach": "tsx src/agents/outreach/cli.ts",
    "response": "tsx src/agents/response/cli.ts",
    "booking": "tsx src/agents/booking/cli.ts",
    "sending": "tsx src/agents/sending/cli.ts",
    "pipeline": "tsx src/orchestrator.ts",
    "pipeline:10": "tsx src/orchestrator.ts 10",
    "pipeline:50": "tsx src/orchestrator.ts 50",
    "pipeline:fast": "tsx src/orchestrator.ts --fast",
    "scheduler": "tsx src/scheduler.ts",
    "scheduler:once": "tsx src/scheduler.ts --once",
    "followups": "tsx src/followup-scheduler.ts",
    "followups:watch": "tsx src/followup-scheduler.ts --watch",
    "test:slack": "tsx src/test-slack.ts",
    "test:classification": "tsx src/test-classification.ts",
    "inbound:server": "tsx src/inbound-webhook.ts",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "start:scheduler": "tsx src/scheduler.ts",
    "start:followups": "tsx src/followup-scheduler.ts --watch",
    "start:webhook": "tsx src/inbound-webhook.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@supabase/supabase-js": "^2.49.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "node-cron": "^4.2.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}